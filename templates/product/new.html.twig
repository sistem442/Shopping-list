{% extends 'base.html.twig' %}
{% block title %}Add Product{% endblock %}
{% block stylesheets %}
    {{ parent() }}
    {{ encore_entry_link_tags('admin') }}
{% endblock %}
{% block body %}
    {% block main %}
    {{ form_start(form) }}
        {{ form_row(form.name) }}
        {{ form_row(form.description) }}
        {{ form_row(form.price) }}
        {{ form_row(form.date) }}
        {{ form_row(form.save) }}

    {{ form_end(form) }}
    {% endblock %}
{% endblock %}